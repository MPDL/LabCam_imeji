


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="example.com.mpdlcamera.Folder.MainActivity"
    android:background="@layout/custom_border">


    <TextView
        android:layout_marginLeft="13dp"
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/logout"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes!"
        android:id="@+id/buttonYes"
        android:layout_marginRight="30dp"
        android:layout_marginEnd="57dp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/buttonNo"
        android:layout_toStartOf="@+id/buttonNo" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No!"
        android:id="@+id/buttonNo"

        android:layout_marginRight="70dp"
        android:layout_marginEnd="108dp"
        android:layout_alignTop="@+id/buttonYes"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2" />
</RelativeLayout>
